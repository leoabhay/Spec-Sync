monetate.c.cr({"data": "console.log(\"Monetate: LIMITED TIME vs TODAY ONLY\");\n\nct_setDDMLimintedTime = function(){\n    // gestisco il tracciamento sull'evento del click delle label\n    var parentElement = document.querySelector('.submenu__items-and-editorial.submenu__items-and-editorial--with-wall-products');\n    const sectionItems = parentElement.querySelectorAll(\".submenu__section:first-child .section__item\");\n    if (sectionItems.length > 0) {\n        sectionItems.forEach(item => {\n            item.addEventListener(\"click\", function() {\n                console.log(\"Monetate: Elemento .section__item cliccato:\", item);\n                window.monetateQ = window.monetateQ || [];\n                window.monetateQ.push([\"trackEvent\",[\"DDMLimintedTime\"]]);\n            });\n        });\n    }\n};\n\nct_setDDMLimitedTimeMobile = function(){\n    // gestisco il tracciamento sull'evento del click delle label mobile\n    var parentElement = document.querySelector('.rb-header__side-panel.rb-header-navigation-submenu-panel.rb-header__side-panel--visible .side-panel__content');\n    const sectionItems = parentElement.querySelectorAll(\".submenu__section:first-child .section__item\");\n    if (sectionItems.length > 0) {\n        sectionItems.forEach(item => {\n            item.addEventListener(\"mouseover\", function() {\n                console.log(\"Monetate: Elemento .section__item cliccato:\", item);\n                window.monetateQ = window.monetateQ || [];\n                window.monetateQ.push([\"trackEvent\",[\"DDMLimintedTimeMobile\"]]);\n            });\n        });\n    }\n};\n\nwindow.ddmPromo = false;\n\n\nif(window.innerWidth < 1150 && document.querySelector('[data-element-id=\"X_X_MainNav_AUPromoCW\"]')) {\n    document.querySelector('[data-element-id=\"X_X_MainNav_AUPromoCW\"]').addEventListener('click', (e) => { \n       \twindow.ddmPromo = true;\n         ct_setDDMLimitedTimeMobile();\n         try {\n               MonetateHelper.reTrack();\n          }catch (error) {\n             console.log('monetate error', error);\n          }    \n    });\n}else if (document.querySelector('[data-element-id=\"X_X_MainNav_AUPromoCW\"]')) {\n    console.log('Monetate: LIMITED TIME vs TODAY ONLY ok elemento');\n    document.querySelector('[data-element-id=\"X_X_MainNav_AUPromoCW\"]').addEventListener('mouseenter', (e) => {\n        window.ddmPromo = true;\n        ct_setDDMLimintedTime();\n        try {\n            MonetateHelper.reTrack();\n            console.log('monetate: retrack');\n        }catch (error) {\n            console.log('monetate error', error);\n        }\n    });\n}\n", "ref": "424/5498913.js"});